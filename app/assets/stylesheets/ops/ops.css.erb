.detailTrace .sourceClass b{color:#cc3300}
.detailTrace .content{padding:10px;line-height:1.7}
.detailTrace .type{margin-right:7px;text-transform:uppercase;color:#18569b}
.detailTrace .error{margin-right:7px;text-transform:uppercase;color:#cc3300}
.detailTrace .warn{margin-right:7px;text-transform:uppercase;color:#e45815}
.detailTrace .success{margin-right:7px;text-transform:uppercase;color:#258b01}

/* touch pad popup style */
.touch_pad_popup .x-btn-default-small .x-btn-inner{font-size:20px;color:#585858;text-shadow:1px 1px #fff;}
.touch_pad_popup .x-btn-default-small-over .x-btn-inner{font-size:24px;color:#fff;font-weight:bold;text-shadow:1px 1px #20395b;}
.touch_pad_popup .x-form-text{font-size:24;}
.touch_pad_popup .x-btn-pressed{background-image:linear-gradient(#20395b 5%,#274369 6%,#497cc0 95%,#30588c 100%,#37649f)}

/* for ie 11 style fix */
.x-nlg .x-toolbar-default{background:none !important;}
.x-nlg #optionbar{background:url(<%= asset_path 'ops/bgTitleBar.png' %>) repeat-x #f3f3f3 !important}