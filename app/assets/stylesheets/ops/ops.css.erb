.pagingToolbar,.x-boundlist .x-toolbar{background:url(<%= asset_path 'ops/bgTh.png' %>) repeat-x;border:1px solid #cfcfcf !important;border-width:0 1px 1px 1px !important;padding:2px 0 2px 5px;margin-top:0}
