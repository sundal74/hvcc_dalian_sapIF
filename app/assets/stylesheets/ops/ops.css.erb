/*topbar style*/
#topbar{padding:0;top:-2px !important}
#topbar .x-btn{background:url(<%= asset_path 'ops/btnSideTail.png' %>) 100% 0 no-repeat;padding:0 20px 0 0;height:27px;margin-right:-15px !important;}
