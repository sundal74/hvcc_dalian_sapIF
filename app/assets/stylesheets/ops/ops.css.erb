.moveright.over button{background-position:-25px 0px}

.x-boundlist{background:url(<%= asset_path 'std/bgCodeCombo.png' %>) 0 100% repeat-x #fff;}
.x-boundlist-item{background:url(<%= asset_path 'std/arrowCodeCombo.png' %>) 7px 50% no-repeat;padding:4px 3px 3px 5px;border:none;border-top:1px solid #fff;border-bottom:1px solid #eee;color:#666;font-size:12px;text-indent:10px;min-height:20px}
