.x-grid-cell{vertical-align:middle}
.x-column-header-trigger{background:url(<%= asset_path 'std/grid3-hd-btn.png' %>) 0 50% no-repeat;width:16px}
.x-grid-row-selected .x-grid-cell-special{border-right:1px solid #6999db}
.x-grid-editor .x-field{margin-top:4px}
