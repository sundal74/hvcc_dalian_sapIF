.x-field-label-cell{vertical-align:middle}
.x-toolbar-docked-bottom{margin-top:10px}
.x-window .x-form-item-label{background:url(<%= asset_path 'ops/dotBlue.png' %>) 3px 50% no-repeat;padding-left:14px}
.x-hbox-form-item + .x-hbox-form-item{border-left:10px solid transparent}
.movedown,.moveup,.moveleft,.moveright{background:none transparent;border:none;padding:0;}
.movedown button,.moveup button,.moveleft button,.moveright button{background:url(<%= asset_path 'ops/btnArrow.png' %>) no-repeat;width:24px;height:24px;}
.moveup button{background-position:0 -50px;margin-left:2px}
