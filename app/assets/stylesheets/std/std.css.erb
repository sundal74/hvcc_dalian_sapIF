.x-form-trigger-wrap-focus .x-form-spinner-down{background-position:100% -65px}
.x-fieldset-header{color:#025eac;font-size:13px;font-weight:bold}
.x-fieldset{background-color:#f6f6f6}
.x-boundlist-selected,.x-boundlist-item-over{color:#fff}
.x-tool img{background-image:url(<%= asset_path 'std/tool-sprites.png' %>);}
.x-form-item-label{min-width:140px}
.x-hbox-form-item + .x-btn{margin-left:3px !important}
.x-menu-item-separator{margin:7px 0;border-top:dotted 1px #83aee3}
.x-border-box .x-grid-editor .x-form-arrow-trigger, .x-border-box .x-grid-row-editor .x-form-arrow-trigger{background-image:url(<%= asset_path 'std/triggerSmall.png' %>)}
.x-grid-editor .x-form-trigger-wrap-focus .x-form-arrow-trigger{background-position:100% -40px}
.x-grid-editor .x-form-trigger-wrap-focus .x-form-arrow-trigger-click{background-position:100% -60px;}
.x-grid-editor .x-form-trigger-wrap .x-form-spinner-up, .x-grid-editor .x-form-trigger-wrap .x-form-spinner-down, .x-grid-row-editor .x-form-trigger-wrap .x-form-spinner-up, .x-grid-row-editor .x-form-trigger-wrap .x-form-spinner-down{background-image:url(<%= asset_path 'std/spinnerSmall.png' %>)}
.x-grid-editor .x-form-trigger-wrap .x-form-spinner-up{margin-top:-10px}
.x-grid-editor .x-form-trigger-wrap-focus .x-form-spinner-up{background-position:100% -40px}
.x-grid-editor .x-form-trigger-wrap-focus .x-form-spinner-down{background-position:100% -50px}
.x-grid-editor .x-form-trigger-wrap .x-form-spinner-up-click{background-position:100% -60px !important}
.x-grid-editor .x-form-trigger-wrap .x-form-spinner-down-click{background-position:100% -70px !important}
.x-fieldset .x-panel-body-default{background:none transparent}
.x-grid-cell-checkcolumn .x-grid-cell-inner{text-align:center !important}
