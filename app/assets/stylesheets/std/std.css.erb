.listSearch .x-btn-over{background-color:#578a1f !important}
.listSearch .x-btn-inner{display:none}
.listSearch .x-field-label-cell{background:url(<%= asset_path 'std/dotGreen.png' %>) 4px 50% no-repeat;text-indent:10px;vertical-align:middle}
.listSearch .x-btn:first-child{background:url(<%= asset_path 'std/iconListSearch.png' %>) 50% 2px no-repeat transparent;border:none}
.listSearch .x-btn:last-child{background:url(<%= asset_path 'std/iconListSearch.png' %>) 50% -23px no-repeat transparent;border:none;left:25px !important}


/*check header style*/
.x-grid-cell-checkcolumn .x-grid-cell-inner{padding-top: 4px;padding-bottom: 2px;line-height: 14px;}
.x-grid-with-row-lines .x-grid-cell-checkcolumn .x-grid-cell-inner {padding-top: 3px;}
.x-grid-checkheader-checked {background:url(<%= asset_path 'std/checked.gif' %>) -32px 0 no-repeat}
.x-grid-checkheader-editor .x-form-cb-wrap {text-align: center;}


/*codePicker style*/
.codePicker{border:2px solid #4f7fcf;background-color:#fff}
.codePicker .x-panel-body-default{border:none}
.codePicker .x-hbox-form-item{margin-top:5px}
.codePicker .x-hbox-form-item + .x-hbox-form-item{border-left:5px solid transparent}
.codePicker .header .x-panel-body-default{padding:0 4px 5px 4px;background-color:#628ac8;}
.codePicker .header input{border:1px solid #2665ac}


/*window style*/
.x-window .x-toolbar-default-docked-bottom{margin-top:3px;padding:3px 15px 12px 0}
.x-window .x-window-body-default{padding:10px 15px}
.x-window-header-default-top,.x-window-body-default{border:none !important;background-color:transparent}
.x-window-header-text-default{font-size:22px;color:#025eac;font-weight:normal;box-shadow:none}
.x-window-header{background:url(<%= asset_path 'std/bgWindow.png' %>) repeat-x;padding:8px 0 7px 12px;border-bottom:1px solid #ccc !important}
.x-window-header .x-window-header-text-container{background:url(<%= asset_path 'std/bgWindowIcon.png' %>) no-repeat;height:37px;padding:10px 0 0 45px}
.x-window-default{border:0;}
.x-window .x-tool-default{min-width:27px}
.x-tool .x-tool-close{background:url(<%= asset_path 'std/iconWindowClose.png' %>) no-repeat !important;width:27px !important;height:27px !important;margin-top:-5px !important;margin-left:-25px !important}


/* StatusBar - structure */
.x-statusbar .x-status-text {cursor: default;}
.x-statusbar .x-status-busy {padding-left: 25px !important;background: transparent no-repeat 3px 0;}
.x-toolbar div.xtb-text .x-statusbar .x-status-text-panel{Border:1px solid;padding: 2px 8px 2px 5px;}
#word-status .x-status-text-panel .spacer {width: 60px;font-size:0;line-height:0;}
#word-status .x-status-busy {padding-left: 25px !important;background: transparent no-repeat 3px 0;}
#word-status .x-status-saved {padding-left: 25px !important; background: transparent no-repeat 3px 0;}
.x-statusbar .x-status-error {cursor: pointer;padding-left: 25px !important;background: transparent no-repeat 3px 0;}
.x-statusbar .x-status-valid {padding-left: 25px !important;background: transparent no-repeat 3px 0;}
.x-status-error-list {font: 11px tahoma,arial,verdana,sans-serif;position: absolute;z-index: 9999;border:1px solid;padding: 5px 10px;}
.x-status-error-list li {cursor: pointer;list-style: disc;margin-left: 10px;}
.x-status-error-list li a {text-decoration: none;}
.x-status-error-list li a:hover {text-decoration: underline;}
#clock{background:url(<%= asset_path 'std/iconClock.png' %>) 0 1px no-repeat;height:24px;text-indent:28px;font-size:13px;line-height:25px;color:#585858}


/* StatusBar - visual */
.x-statusbar .x-status-busy {background-image: url(<%= asset_path 'std/loading.gif' %>);}
.x-statusbar .x-status-text-panel {border-color: #99bbe8 #fff #fff #99bbe8;}
#word-status .x-status-text{color: #777;}
#word-status .x-status-busy{background-image: url(<%= asset_path 'std/saving.gif' %>);}
#word-status .x-status-saved{background-image: url(<%= asset_path 'std/saved.png' %>);}
.x-statusbar .x-status-error{color:#C33;background-image: url(<%= asset_path 'std/exclamation.gif' %>);}
.x-statusbar .x-status-valid{background-image: url(<%= asset_path 'std/accept.png' %>);}
.x-status-error-list{border-color:#C33;background: white;}
.x-status-error-list li a{color: #15428B;}

.statusBar{background:url(<%= asset_path 'std/bgSouth.png' %>) repeat-x #f4f4f4;padding-left:7px}

/*tooltip style*/
.x-tip{background-color:#c46f6f;border:none}
.x-tip-header, .x-tip-body, .x-form-invalid-tip-body{color:#fff;font-size:12px}
.x-tip-body, .x-form-invalid-tip-body{padding:2px 3px}

/*trace detail style*/
.detailTrace{border:1px solid #d8d8d8}
.detailTrace .title{background:url(<%= asset_path 'std/iconTrace.png' %>) 8px 9px no-repeat #f1f1f1;padding:3px 7px 5px 7px;text-indent:40px;font-size:18px;font-weight:bold}
.detailTrace .title .info{display:block;font-size:11px;color:#666;font-weight:normal}
.detailTrace .sourceClass{border:1px solid #bbb;border-width:1px 0;padding:1px 10px;background:#cfcfcf;color:#111}
.detailTrace .sourceClass span{display:block;line-height:1.5}
.detailTrace .sourceClass b{color:#cc3300}
.detailTrace .content{padding:10px;line-height:1.7}
.detailTrace .type{margin-right:7px;text-transform:uppercase;color:#18569b}
.detailTrace .error{margin-right:7px;text-transform:uppercase;color:#cc3300}
.detailTrace .warn{margin-right:7px;text-transform:uppercase;color:#e45815}
.detailTrace .success{margin-right:7px;text-transform:uppercase;color:#258b01}

/*file upload*/
.new_attachment{background:url(<%= asset_path 'std/iconFileUpload.png' %>) no-repeat;line-height:25px;text-indent:26px;height:25px}
.new_attachment label{margin-right:5px;font-weight:bold}


/* for ie 11 style fix */
.x-nlg .x-toolbar-default{background:none !important;}
.x-nlg .titlebar{background:url(<%= asset_path 'std/bgTitleBar.png' %>) repeat-x !important;}
.x-nlg .navBar .x-toolbar{background:url(<%= asset_path 'std/bgTitle.png' %>) 100% 50% no-repeat #7db019 !important;}
.x-nlg .x-tab-default-top{background:none #f4f4f4 !important;}
.x-nlg .x-tab-top-active{background:none #fff !important;}
.x-nlg .x-panel-header-default-top{background:none !important;}
.x-nlg .x-datepicker-footer, .x-nlg .x-monthpicker-buttons{background:none !important;}