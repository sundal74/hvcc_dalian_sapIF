.pagingToolbar .x-btn .x-btn-icon,.x-boundlist .x-toolbar .x-btn .x-btn-icon{background-image:url(<%= asset_path 'ops/btnPagingBar.png' %>) !important}
.pagingToolbar .x-tbar-page-first,.x-boundlist .x-tbar-page-first{background-position:0 0px}
.pagingToolbar .x-tbar-page-prev,.x-boundlist .x-tbar-page-prev{background-position:0 -16px}
.pagingToolbar .x-tbar-page-next,.x-boundlist .x-tbar-page-next{background-position:0 -32px}
