#topbar .x-btn-inner{color:#fff;text-align:left}
#topbar .sideLogout{margin-right:0 !important}
#topbar .x-btn-arrow{background:url(<%= asset_path 'std/bgMenuArrow.png' %>) 100% 0 no-repeat;height:27px}



/*title bar style*/
.titlebar{background:url(<%= asset_path 'std/bgTitleBar.png' %>) repeat-x;border-bottom:1px solid #dedede !important;text-indent:10px}
.titlebar label{color:#025eac;font-size:14px}
.btnCollapseSearch,.btnClose,.btnCollapseSidebar{background:none;border:0;padding:0;border-left:1px solid #e4e4e4;}
.btnCollapseSearch button,.btnClose button,.btnCollapseSidebar button{background:url(<%= asset_path 'std/iconTitleBar.png' %>) no-repeat;width:30px;height:30px;opacity:.7;}
.titlebar .x-btn-over button{opacity:1}
.btnCollapseSearch button{background-position:0 -152px;}
.btnClose button{background-position:0 -2px;}
.btnCollapseSidebar button{background-position:0 -52px;}
.btnCollapseSearch .x-btn-inner,.btnClose .x-btn-inner{font-size:0px}


/*nav style*/
.menuTab{padding:0}
.menuTab .x-btn-default-toolbar-small{background:none;border:none;padding:0 8px;border-radius:0;height:30px}
.menuTab .x-btn-default-toolbar-small .x-btn-inner{font-size:15px;color:#9ac1f1}
.menuTab .x-btn-default-toolbar-small-over .x-btn-inner,.menuTab .active .x-btn-inner{color:#fff;}
.userMenu .x-btn-default-toolbar-small{padding:2px 1px;width:94%;margin:1px 0 0 2px !important}

.userMenu .x-box-inner{height:24px !important}
.iconUserMenu{background:url(<%= asset_path 'std/iconUserMenu.png' %>) no-repeat;margin-left:5px}
.userMenu .x-btn-default-toolbar-small-icon-text-left .x-btn-inner{padding-left:22px;line-height:1.7}
.userMenu .x-btn-arrow{background:url(<%= asset_path 'std/btnArrowDown.png' %>) 99% -1px no-repeat;}
.userMenu .x-btn-over .x-btn-inner{color:#000;}


/*menu style*/
#navcnt{padding-top:30px !important}
.navBar	.x-toolbar{background:url(<%= asset_path 'std/bgTitle.png' %>) 100% 50% no-repeat #7db019;height:40px}
.navBar	.x-toolbar	label{color:#fff;font-size:16px;text-indent:28px;line-height:39px}
.navbar .x-panel-body-default{border:0;top:40px !important}
.userMenu{background:url(<%= asset_path 'std/bgTitleBarBlue.png' %>) 100% 0 no-repeat;height:30px !important}
.menu-item{display:block;border-top:1px solid transparent;border-bottom:1px solid #efefef;padding:4px 7px;cursor:pointer;color:#666;line-height:1.5;font-size:12px}
.menu-item:hover,.menu-item-hover{background:url(<%= asset_path 'std/bgSubmenuOver.png' %>) 155px 50% no-repeat;color:#326db2;font-weight:bold}
.menu-item-active{background:url(<%= asset_path 'std/bgSubmenuActive.png' %>) 155px 50% no-repeat #326db2 !important;color:#fff !important;font-weight:bold !important}
.menu-item-separator{display:block;padding:2px 7px;border-top:1px solid #c1dea5;border-bottom:1px solid #d8ecc5;background:#e9f3dc;color:#4c8f0a;font-size:14px;font-weight:bold}


/*sidebar style*/
.brand{position:absolute;top:-3px;margin-left:10px;color:#fff;font-size:21px;font-weight:bold}
.brand span{display:block;color:#ccc;font-size:11px;font-weight:normal}
.sidebar .x-panel-header-default,.sidebar .x-panel-body-default{border:none;background:none}
#sidebar-body{padding:12px 10px 0 10px}


/*grid style*/
.x-column-header{background:url(<%= asset_path 'std/bgTh.png' %>) repeat-x;font-size:12px;color:#585858;line-height:17px;text-align:center}
.x-column-header-over{background:url(<%= asset_path 'std/bgThOver.png' %>) repeat-x;font-size:12px;color:#1b68c8;}
