.x-form-trigger-wrap .x-form-spinner-up{margin-top:-17px}
.x-form-trigger-wrap .x-form-spinner-up-over{background-position:100% -35px}
.x-form-trigger-wrap .x-form-spinner-up-click{background-position:100% -105px !important}
.x-form-trigger-wrap-focus .x-form-spinner-up{background-position:100% -70px}
.x-form-trigger-wrap .x-form-spinner-down{background-position:100% -17px}
.x-form-trigger-wrap .x-form-spinner-down-over{background-position:100% -52px}
