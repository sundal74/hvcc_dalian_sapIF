/*form style*/
.x-form-checkbox,.x-grid-checkheader,.x-grid-row-checker, .x-column-header-checkbox .x-column-header-text{background:url(<%= asset_path 'std/iconCheckBox.png' %>) -16px 0 no-repeat;width:16px;height:16px;margin:auto}

