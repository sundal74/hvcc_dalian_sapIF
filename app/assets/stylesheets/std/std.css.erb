.x-form-arrow-trigger-click{background-position:100% -78px;}
.x-form-trigger-wrap-focus .x-form-arrow-trigger{background-position:100% -52px;}
.x-trigger-cell{width:20px !important}
.x-form-date-trigger{background:url(<%= asset_path 'std/date-trigger.png' %>) 100% 0 no-repeat;width:23px;position:absolute;margin-left:-3px;border-bottom:none}
.x-form-date-trigger-over{background-position:100% -26px;}
.x-form-date-trigger-click{background-position:100% -78px;}
.x-form-trigger-wrap-focus .x-form-date-trigger{background-position:100% -52px;}
