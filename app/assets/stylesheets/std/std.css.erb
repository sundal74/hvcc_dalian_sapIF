/*data picker style*/
.x-datepicker{padding:5px 5px 0 5px;width:200px !important;border:1px solid #bbb;min-height:178px}
.x-datepicker .x-datepicker-header{background:url(<%= asset_path 'std/bgCalendarY.png' %>) repeat-x;border:1px solid #a2a5aa}
.x-monthpicker-yearnav{background:url(<%= asset_path 'std/bgCalendarY.png' %>) repeat-x;}
table.x-datepicker-inner th{background:url(<%= asset_path 'std/bgCalendarTop.png' %>) repeat-x;border-bottom:1px solid #acabb3}
table.x-datepicker-inner td{border-left:1px solid #dcdce1;border-bottom:1px solid #dcdce1;background-color:#f6f6f6}
table.x-datepicker-inner tr td:first-child{border-left:none}
.x-datepicker-prevday{background-color:#e2e2e2 !important;border-top:1px solid #cfcfcf !important;border-right:1px solid #cfcfcf !important}
.x-datepicker-today,.x-datepicker-selected,.x-monthpicker-selected{background:url(<%= asset_path 'std/bgCalendarToday.png' %>) repeat-x !important;border:1px solid #1d5388 !important}
table.x-datepicker-inner a:hover, table.x-datepicker-inner .x-datepicker-disabled a:hover{background-color:transparent}
table.x-datepicker-inner .x-datepicker-today a{border:none;}
table.x-datepicker-inner .x-datepicker-selected a{background-color:transparent !important;border:none}
.x-datepicker-today span,.x-datepicker-selected span,.x-monthpicker-selected{color:#fff !important}
.x-datepicker-month span{color:#5f6068 !important}
.x-datepicker-date:hover span{color:#025eac !important}
.x-datepicker-prev a,.x-monthpicker-yearnav-prev{background:url(<%= asset_path 'std/btnCalendarLeft.png' %>) no-repeat !important;}
.x-datepicker-next a,.x-monthpicker-yearnav-next{background:url(<%= asset_path 'std/btnCalendarRight.png' %>) no-repeat !important;}
.x-datepicker-inner{border:1px solid #acabb3;border-width:0 1px 1px 1px}
.x-datepicker-footer,.x-monthpicker-buttons{background:none;border:none}
.x-datepicker .x-form-item-label{min-width:35px}
.x-datepicker .x-form-text{padding:1px 2px}
.x-datepicker .ux-timefield .x-field-default{margin-top:2px}
.x-datepicker .ux-timefield .x-form-item-body table{margin-right:3px}
.x-datepicker .ux-timefield .x-form-item-body{min-width:130px !important}
.x-datepicker .x-form-trigger-wrap .x-form-spinner-up,.x-datepicker .x-form-trigger-wrap .x-form-spinner-down{margin-left:-5px}
.x-monthpicker-item{border-left:1px solid #dcdce1;border-bottom:1px solid #dcdce1;background-color:#f6f6f6;margin:0;height:28px;width:49px !important}
.x-monthpicker-months .x-monthpicker-item:nth-child(odd){border-left:none}
.x-monthpicker-years .x-monthpicker-item:nth-child(even){border-left:none}
.x-monthpicker-item a{border:0;padding:5px 0 0 0;height:28px;margin:0}
.x-monthpicker-months,.x-monthpicker-years{width:50%}
.x-monthpicker-months{border-right:1px solid #ccc}
.x-monthpicker-item a:hover{color:#fff}
.x-datepicker-month .x-btn-split-right{background-image:url(<%= asset_path 'std/s-arrow-light.png' %>)}


/*progress bar style*/
.x-progress{width:170px;height:7px;border:1px solid #fff}
.x-progress-text{display:none}
.x-progress-default .x-progress-bar{background:none;border:none;background-color:#3670c7}
.btnTracer{background:none;border:none;background:url(<%= asset_path 'std/iconTrace.png' %>) 50% 50% no-repeat;width:30px;height:25px;margin-left:3px !important;margin-right:3px !important}
.btnTracer .x-btn-inner{display:none}


/*topbar*/
#topbar{padding:0;top:-2px !important}
#topbar .x-btn{background:url(<%= asset_path 'std/btnSideTail.png' %>) 100% 0 no-repeat;padding:0 20px 0 0;height:27px;margin-right:-15px !important;}
#topbar button{background:url(<%= asset_path 'std/btnSideBody.png' %>) no-repeat;height:27px;padding:0 0 3px 20px}
#topbar .sideOPS{background:url(<%= asset_path 'std/btnSideLinkTail.png' %>) 100% 0 no-repeat;padding:0 20px 0 0;height:27px;margin-right:-15px !important;}
#topbar .sideOPS button{background:url(<%= asset_path 'std/btnSideLinkBody.png' %>) no-repeat;height:27px;padding:0 0 3px 20px}
#topbar .x-btn:nth-child(1){z-index:100}
#topbar .x-btn:nth-child(2){z-index:90}
#topbar .x-btn:nth-child(3){z-index:80}
