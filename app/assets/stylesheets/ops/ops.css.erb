#titlebar label{background:url(<%= asset_path 'ops/iconTitle.png' %>) 0 2px no-repeat;margin:5px 0 0 0px !important;font-size:20px;font-weight:bold;color:#111;text-indent:22px}


/* navbar style */
#navbar{background-color:#fff;border-right:1px dotted #e0e0e0 !important}
