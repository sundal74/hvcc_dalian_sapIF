.x-boundlist-selected,.x-boundlist-item-over{color:#fff}
.x-tool img{background-image:url(<%= asset_path 'ops/tool-sprites.png' %>);}
.x-form-item-label{min-width:180px;font-size:16px}
.x-hbox-form-item + .x-btn{margin-left:3px !important}
